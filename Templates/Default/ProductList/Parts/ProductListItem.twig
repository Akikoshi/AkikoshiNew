<div class="col-xs-12 col-sm-4 col-md-4 col-lg-3">
	<div class="thumbnail" style="min-height: 350px;">
		<img src="{{ productItem.getPictureUrl }}" alt="bild">
		<div class="caption">
			<h3>{{ productItem.getName }}</h3>
			<p>{{ productItem.getDescription }}</p>
			<p>{{ productItem.getPrice }} Euro</p>
			<p><a href="{{ productItem.getDetailUrl() }}" class="btn btn-primary>" role="button">Produktdetails</a></p>
		</div>
	</div>
</div>